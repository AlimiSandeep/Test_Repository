package com.cg.stream.demos;

import java.util.Arrays;
import java.util.List;

public class MapDemo1 {
public static void main(String[] args) {
	List<String> list=Arrays.asList("ram","babu","anand","vikas","giri");
	
	
	System.out.println("Display first two letters of a string in upper case and should end with 2019");
	list.stream().map(e->e.substring(0, 2).toUpperCase()+"2019").forEach(System.out::println);
	
}
}
