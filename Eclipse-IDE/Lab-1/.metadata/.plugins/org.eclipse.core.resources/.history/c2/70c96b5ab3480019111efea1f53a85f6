package com.cg.Demo;

import java.util.Scanner;

public class SecondSmallest {
public static void main(String[] args) {
	Scanner scan=new Scanner(System.in);
	int n=scan.nextInt();
	int arr[]=new int[n];
	for(int i=0;i<n;i++)
		arr[i]=scan.nextInt();
	System.out.println(getSecondSmallest(arr));
}
}
