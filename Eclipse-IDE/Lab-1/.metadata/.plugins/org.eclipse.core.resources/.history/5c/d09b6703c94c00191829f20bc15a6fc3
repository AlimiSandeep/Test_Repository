package com.cg.wrapper;

public class ParseStringDemo {
	public static void main(String[] args) {
		
	
	String str1="25";
	
	//Convert string to int primitive type
	int value=Integer.parseInt(str1);
	System.out.println(value);
	
	String str2="25.56";

	
	double value1=Double.parseDouble(str2);
	System.out.println(value1);
	}

}
